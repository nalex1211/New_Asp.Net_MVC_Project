@model ApplicationUser

<div class="NoteTable p-3 mt-4">
    <form>
        <div class="mb-3">
            <label asp-for="FristName" style="font-size:30px">Имя</label>
            <label asp-for="FristName" style="font-size:30px;border:0px" class="form-control">@Model.FristName</label>
        </div>

        <div class="mb-3">
            <label asp-for="LastName" style="font-size:30px">Фамилия</label>
            <label asp-for="LastName" style="font-size:30px;border:0px" class="form-control">@Model.LastName</label>
        </div>

        <div class="mb-3">
            <label asp-for="UserName" style="font-size:30px">Никнейм</label>
            <label asp-for="UserName" style="font-size:30px;border:0px" class="form-control">@Model.UserName</label>
        </div>

        <button style="font-size:30px; border-collapse: separate;border-radius: 7px;border-style: solid" class="btn btn-primary" asp-for="" asp-controller="Dashboard" asp-action="EditProfile">Редактировать профиль</button>
        <button style="font-size:30px; border-collapse: separate;border-radius: 7px;border-style: solid;background-color:#3B3B3B" class="btn btn-primary" asp-for="" asp-controller="Note" asp-action="AllNotes">Вернуться на главную страницу</button>
    </form>
</div>
